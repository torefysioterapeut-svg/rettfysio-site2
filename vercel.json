{
  "version": 2,
  "builds": [{ "src": "index.html", "use": "@vercel/static" }],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.rettfysio.no" }],
      "destination": "https://rettfysio.no/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "rettfysio-site2.vercel.app" }],
      "destination": "https://rettfysio.no/$1",
      "permanent": true
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/index.html" }]
}

